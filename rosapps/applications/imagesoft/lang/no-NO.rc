  LTEXT "bilde størrelse:", IDC_STATIC, 12, 125, 42, 9
  LTEXT "", IDC_IMAGE_SIZE, 58, 125, 54, 9
  PUSHBUTTON "OK", IDOK, 50, 144, 48, 13
  PUSHBUTTON "Avbryt", IDCANCEL, 102, 144, 48, 13
END

