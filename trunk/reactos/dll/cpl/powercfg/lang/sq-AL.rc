    GROUPBOX "Hibernate", -1, 6, 41, 240, 32
    CONTROL "Mundëso &hibernation", IDC_HIBERNATEFILE, "Button", BS_AUTOCHECKBOX | WS_TABSTOP, 17, 55, 210, 10
