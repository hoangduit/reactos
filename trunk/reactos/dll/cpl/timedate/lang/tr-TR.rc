    LTEXT "", IDC_SUCSYNC, 12, 54, 214, 23
    LTEXT "", IDC_NEXTSYNC, 12, 96, 137, 12
    LTEXT "Eşleme, yalnızca bilgisayar Umûmî Ağ'a bağlandığında olur.", -1, 12, 114, 225, 25
END

