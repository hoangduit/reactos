    PUSHBUTTON "向上", IDC_DNSADDRUP, 190, 30, 50, 14, WS_TABSTOP
    PUSHBUTTON "向下", IDC_DNSADDRDOWN, 190, 50, 50, 14, WS_TABSTOP
