LANGUAGE LANG_GERMAN, SUBLANG_NEUTRAL

IDR_MAINMENU MENU
BEGIN
    POPUP "&Datei"
    BEGIN
        MENUITEM "Ö&ffnen...", IDM_OPEN_FILE
        MENUITEM "Sch&ließen", IDM_CLOSE_FILE
        MENUITEM SEPARATOR
        MENUITEM "&Beenden", IDM_EXIT
    END
    POPUP "&Gerät"
    BEGIN
        MENUITEM "&Eigenschaften", IDM_DEVPROPS
        MENUITEM SEPARATOR
        MENUITEM "&Lautstärkeregelung", IDM_VOLUMECTL
    END
    POPUP "&Hilfe"
    BEGIN
        MENUITEM "&Über...", IDM_ABOUT
    END
END

