/* Strings */

STRINGTABLE
BEGIN
    IDS_SOL_NAME "Solitaire"
    IDS_SOL_ABOUT "Solitaire, J Brown eliyle yapılmıştır.\n\nCardLib sürümü: 1.0"
    IDS_SOL_QUIT "Bu oyundan çıkılsın mı?"
