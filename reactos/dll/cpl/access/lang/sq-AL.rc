                 PROPSHEETPADDING(2), LABELLINE(12), PROPSHEETWIDTH - PROPSHEETPADDING(4), 14
    GROUPBOX "Njoftim", -1, PROPSHEETPADDING(1), LABELLINE(15), PROPSHEETWIDTH - PROPSHEETPADDING(2), LABELLINE(4) + PROPSHEETPADDING(1)
