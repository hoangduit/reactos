    PUSHBUTTON "Správce úloh", IDC_TASKMGR, 90, 154, 70, 14
    PUSHBUTTON "Storno", IDCANCEL, 170, 154, 70, 14
END

IDD_LOCKED_DLG DIALOGEX 0, 0, 275, 121
