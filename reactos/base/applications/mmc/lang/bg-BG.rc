        MENUITEM "New\tStrg+N", IDM_FILE_NEW
        MENUITEM SEPARATOR
        MENUITEM "Из&ход\tAlt+F4", IDM_FILE_EXIT
    END

    POPUP "Помо&щ"
    BEGIN
        MENUITEM "&За Прибора за управление на РеактОС...", IDM_HELP_ABOUT
    END
END

IDM_CONSOLE_LARGE MENU
BEGIN
