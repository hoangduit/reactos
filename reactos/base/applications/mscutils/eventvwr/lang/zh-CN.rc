    IDS_COLUMNTYPE "类型"
    IDS_COLUMNDATE "日期"
    IDS_COLUMNTIME "时间"
    IDS_COLUMNSOURCE "来源"
    IDS_COLUMNCATEGORY "分类"
    IDS_COLUMNEVENT "事件"
    IDS_COLUMNUSER "用户"
